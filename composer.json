{
    "name": "dara/slimmy",
    "description": "A RestFul API using Slim for a NaijaEmoji Service",
    "keywords": [
        "API",
        "RestFul",
        "Emoji",
        "NaijaEmoji"
    ],
    "homepage": "https://github.com/andela-doladosu/slimmy",
    "license": "MIT",
    "authors": [
        {
            "name": "Dara Oladosu",
            "email": "dara.Oladosu@andela.com",
            "homepage": "http://github.com/andela-doladosu",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "slim/slim": "^2.6",
        "dara/potatoorm": "dev-master",
        "phpspec/phpspec": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Dara\\": "src"
        },
        "classmap": ["src"]
    },
    "autoload-dev": {
    "classmap":[
      "spec/"
    ],
    "spec\\": "spec/"
    },
    "scripts": {
        "test": "phpunit",
        "pre-update-cmd": [
            "composer clear-cache"
        ],
        "pre-install-cmd": [
            "composer clear-cache"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
